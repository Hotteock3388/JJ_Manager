<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="MyPageTitle" parent="TextView_Black">
        <item name="android:textSize">18dp</item>
        <item name="android:fontFamily">@font/notosanskr_bold</item>
    </style>

    <style name="MyPageItem" parent="TextView_Black">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">15dp</item>
        <item name="android:background">@color/white</item>
        <item name="android:foreground">?attr/selectableItemBackground</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="android:fontFamily">@font/notosanskr_medium</item>
    </style>

    <style name="MyPageBGImageView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">300dp</item>
    </style>

</resources>